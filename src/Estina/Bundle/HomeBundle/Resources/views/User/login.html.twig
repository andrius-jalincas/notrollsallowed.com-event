{% if error %}
    <div>{{ error.messageKey|trans(error.messageData) }}</div>
{% endif %}

<form action="{{ path('login_check') }}" method="post">

    {{ form_row(loginForm._username) }}
    {{ form_row(loginForm._password) }}
    {{ form_rest(loginForm) }}

    <button type="submit">login</button>
</form>